import flash.ui.*;import flash.events.*;import flash.net.*;const AUTHOR = 'Karbon Inc.';var date:Date = new Date();var byMe:ContextMenuItem = new ContextMenuItem(":: site by " + AUTHOR + " ::");var copyright:ContextMenuItem = new ContextMenuItem("© " + date.getFullYear() + " ClientsName", true);byMe.addEventListener(ContextMenuEvent.MENU_ITEM_SELECT, itemClicked);var menu:ContextMenu = new ContextMenu();menu.hideBuiltInItems();menu.customItems.push(byMe, copyright)this.contextMenu = menu;function itemClicked(Event:ContextMenuEvent):void{	navigateToURL(new URLRequest("urlGoesHere?subject=subjectGoesHere"), "_blank");}